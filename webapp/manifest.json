{
    "_version": "1.12.0",
    "sap.app": {
      "id": "wdi5.test",
      "type": "application",
      "applicationVersion": {
        "version": "0.0.1"
      }
    },
    "sap.ui5": {
      "routing": {
        "config": {
          "routerClass": "sap.m.routing.Router",
          "viewType": "XML",
          "async": true,
          "viewPath": "wdi5.test.view",
          "controlAggregation": "pages",
          "controlId": "app",
          "clearControlAggregation": false
        },
        "routes": [
          {
            "name": "Main",
            "pattern": "RouteMain",
            "target": [
              "TargetMain"
            ]
          }
        ],
        "targets": {
          "TargetMain": {
            "viewType": "XML",
            "transition": "slide",
            "clearControlAggregation": false,
            "viewId": "Main",
            "viewName": "Main"
          }
        }
      },
      "rootView": {
        "viewName": "wdi5.test.view.Main",
        "type": "XML",
        "async": true,
        "id": "app"
      }
    }
  }